{
  "name": "dot-graph",
  "version": "0.0.1",
  "description": "This is a simple dot-matrix Graph",
  "license": "TRUST",
  "author": "TURYA BISWAS",
  "keywords": [
    "graph",
    "dot graph",
    "dot-graph",
    "react-dot-graph",
    "react graph"
  ],
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/my-component.umd.js",
  "module": "./dist/my-component.es.js",
  "exports": {
    ".": {
      "import": "./dist/my-component.es.js",
      "require": "./dist/my-component.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "devDependencies": {
    "@testing-library/dom": "^8.20.0",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.32.1",
    "jsdom": "^21.1.0",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.4",
    "vitest": "^0.28.1"
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  }
}
